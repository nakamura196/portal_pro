    <rdfs:label xml:lang="en">Skill</rdfs:label>
    <rdfs:label xml:lang="it">Abilità</rdfs:label>
    <rdfs:label xml:lang="zh">技能</rdfs:label>
    <rdfs:label xml:lang="ar">مهارة</rdfs:label>
    <rdfs:label xml:lang="nl">Vaardigheid</rdfs:label>
    <rdfs:label xml:lang="pt">Habilidade</rdfs:label>
    <rdfs:label xml:lang="ru">Навык</rdfs:label>
    <rdfs:label xml:lang="ja">スキル</rdfs:label>
    <rdfs:label xml:lang="fr">Habileté (technique)</rdfs:label>
