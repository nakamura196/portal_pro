    <rdfs:label xml:lang="ja">省</rdfs:label>
    <rdfs:label xml:lang="ru">Министерство</rdfs:label>
    <rdfs:label xml:lang="nl">Ministerie</rdfs:label>
    <rdfs:label xml:lang="de">Ministerium</rdfs:label>
    <rdfs:label xml:lang="ar">وزارة</rdfs:label>
    <rdfs:label xml:lang="es">Ministerio</rdfs:label>
    <rdfs:label xml:lang="pt">Ministério</rdfs:label>
