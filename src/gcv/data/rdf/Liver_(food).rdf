    <rdfs:label xml:lang="fr">Foie (alimentation)</rdfs:label>
    <rdfs:label xml:lang="it">Fegato (alimento)</rdfs:label>
    <rdfs:label xml:lang="en">Liver (food)</rdfs:label>
    <rdfs:label xml:lang="de">Leber (Lebensmittel)</rdfs:label>
    <rdfs:label xml:lang="es">Hígado (gastronomía)</rdfs:label>
