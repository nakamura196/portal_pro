    <rdfs:label xml:lang="ja">三井三池炭鉱</rdfs:label>
    <rdfs:label xml:lang="en">Miike coal mine</rdfs:label>
    <rdfs:label xml:lang="es">Mina de carbón de Miike</rdfs:label>
